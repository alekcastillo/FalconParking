<Window x:Class="FalconParkingClient.ReserveSlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FalconParkingClient"
        mc:Ignorable="d"
        Title="Reservar campo" Height="186" Width="293">
    <Grid>
        <Label x:Name="lblLicensePlate" Content="Tiempo de reservacion" HorizontalAlignment="Left" Margin="26,56,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblTitle" Content="Reservar campo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="18"/>
        <ComboBox x:Name="cboxTime" HorizontalAlignment="Left" Margin="26,86,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem>15 minutos</ComboBoxItem>
            <ComboBoxItem>Media hora</ComboBoxItem>
            <ComboBoxItem>Una hora</ComboBoxItem>
        </ComboBox>
        <Button x:Name="btnReserve" Content="Reservar" Margin="0,0,10,9" HorizontalAlignment="Right" Width="54" Height="19" VerticalAlignment="Bottom" Click="btnLogin_Click"/>
    </Grid>
</Window>
