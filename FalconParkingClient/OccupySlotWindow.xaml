<Window x:Class="FalconParkingClient.OccupySlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FalconParkingClient"
        mc:Ignorable="d"
        Title="Ocupar campo" Height="239" Width="293">
    <Grid>
        <Label x:Name="lblLicensePlate" Content="Numero de placa" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtLicensePlate" HorizontalAlignment="Center" Margin="0,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"/>
        <Label x:Name="lblUserIdentification" Content="Cedula del conductor" HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUserIdentification" HorizontalAlignment="Center" Margin="0,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="btnOccupy" Content="Ocupar" Margin="0,0,10,9" HorizontalAlignment="Right" Width="43" Height="19" VerticalAlignment="Bottom" Click="btnLogin_Click"/>
        <Label x:Name="lblTitle" Content="Ocupar campo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="18"/>

    </Grid>
</Window>
