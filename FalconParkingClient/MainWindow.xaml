<Window x:Class="FalconParkingClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FalconParkingClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="650">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="86*"/>
            <ColumnDefinition Width="239*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="BtnConnect" Content="Connect" HorizontalAlignment="Center" Margin="0,27,0,0" VerticalAlignment="Top" Height="36" Width="127" Click="BtnConnect_Click"/>
        <TabControl x:Name="tabctlLots" Grid.Column="1">
        </TabControl>
    </Grid>
</Window>
